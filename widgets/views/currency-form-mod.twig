<div id="currency">
    <ul class="currency-list">
        <li class="currency-button"><span>{{ current.code }}</span>
            <ul class="currency-not-active disable" data-link="{{ link }}" data-action="{{ action }}" data-token="{{ app.request.csrfToken }}">
                {% for item in currency %}
                    <li data-id="{{ item.id }}"><span>{{ item.code }}</span></li>
                {% endfor %}
            </ul>
        </li>
    </ul>
</div>
