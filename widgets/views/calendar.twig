<div class="calendar">
    <table border="1">
        <tr>
            {% for dayName in dayNames %}
                <td>{{ dayName }}</td>
            {% endfor %}
        </tr>
        {% for week in month %}
            <tr>
                {% for day in week %}
                    <td data="{{ day.timestamp }}">{{ day.number }}</td>
                {% endfor %}
            </tr>
        {% endfor %}
    </table>
</div>
